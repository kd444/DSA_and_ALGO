    "def sortedSquaredArray(array):\n",
    "    sortedSquares=[0 for _ in array]\n",
    "\n",
    "    for idx in range(len(array)):\n",
    "        value= array[idx]\n",
    "        sortedSquares[idx] = value*value\n",
    "    sortedSquares.sort()\n",
    "    return sortedSquares  "
   ]
  }
