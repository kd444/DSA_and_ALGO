    "def maxProfitI(self,prices):\n",
    "    l,r = 0,1\n",
    "    maxP=0\n",
    "    while r < len(prices):\n",
    "        if prices[l]<prices[r]:\n",
    "            profit = prices[r] - prices[l]\n",
    "            maxP = max(maxP,profit)\n",
    "        else:\n",
    "            l=r\n",
    "        r +=1\n",
    "    return maxP\n"
   ]
