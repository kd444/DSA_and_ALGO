    "def countBits(self, n):\n",
    "    dp= [0]*(n+1)\n",
    "    offset =1\n",
    "    \n",
    "    for i in range(1,n+1):\n",
    "        if offset*2==i:\n",
    "            offset=i\n",
    "        dp[i]= 1+ dp[i -offset]\n",
    "    return dp"
   ]
