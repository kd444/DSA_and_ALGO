    "def rob(self,nums):\n",
    "    rob1,rob2=0,0\n",
    "    # [rob1,rob2,n,n+1,...]\n",
    "    for n in nums:\n",
    "        temp=max(n+rob1,rob2)\n",
    "        rob1= rob2\n",
    "        rob2= temp\n",
    "    return rob2    "
