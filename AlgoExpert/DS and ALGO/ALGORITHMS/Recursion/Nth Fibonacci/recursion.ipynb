    "class Solution(object):\n",
    "    def fib(self, n):\n",
    "        if n==0:\n",
    "            return 0\n",
    "        if n==1:\n",
    "            return 1\n",
    "        else:\n",
    "            return self.fib(n-1) + self.fib(n -2 )       \n",
    "\n",
